        <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="true">
          <stringProp name="BeanShellAssertion.query">File file = new File(&quot;${testdir}/Fundamentals/Data/Out/${targetFileName}.txt&quot;);

//check file existence
if (!file.exists())
{
    	Failure = true;
    	FailureMessage = &quot;File &quot; + file.getName() + &quot; does not exist&quot;;
}else{

	//check file size
	long expectedSize1 = 15127;  // 2922 characters for Windows (including line separator character)
	//long expectedSize2 = 2883;  // 2883 characters for Linux (including line separator character)
	long actualSize = file.length();
	
	if (expectedSize1 != actualSize){
		Failure = true;
		FailureMessage = &quot;### Actual file size differs from expected. Expected: &quot; + expectedSize1 + &quot; On Windows, or &quot; + 				    expectedSize2 + &quot; On Linux, but got: &quot; + actualSize;
	
	}else{
		log.info(&quot;## Actual File size is excatly same as that of Expected Actual | Expected - &quot; + actualSize +&quot; | &quot;+ expectedSize1);	
		
	}
	
}

</stringProp>
          <stringProp name="BeanShellAssertion.filename"></stringProp>
          <stringProp name="BeanShellAssertion.parameters"></stringProp>
          <boolProp name="BeanShellAssertion.resetInterpreter">false</boolProp>
        </BeanShellAssertion>
